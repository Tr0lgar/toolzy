{
  "name": "toolzy",
  "version": "0.1.2",
  "description": "A lightweight utility toolkit for JS/TS projects.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "utils",
    "toolkit",
    "typescript",
    "random",
    "string",
    "helpers"
  ],
  "author": "tr0lgar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tr0lgar/toolzy.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "generate-doc": "node --loader ts-node/esm scripts/generate-doc-html.mts"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsup": "8.3.5",
    "typescript": "^5.8.3",
    "vitest": "^1.0.0"
  },
  "private": false
}
